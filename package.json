{
  "name": "legal-consular-system",
  "version": "1.0.0",
  "description": "领事认证管理系统",
  "main": "index.js",
  "scripts": {
    "build": "cd frontend && npm install && npm run build",
    "install-all": "npm install && cd frontend && npm install",
    "build-all": "cd frontend && npm run build",
    "start-backend": "cd backend && npm start",
    "start-frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "setup": "npm run install-all && npm run build-all"
  },
  "keywords": ["legal", "consular", "management", "system"],
  "author": "Yur Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@vercel/node": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "sqlite3": "^5.1.6"
  }
}
