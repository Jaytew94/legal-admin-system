{
  "version": 2,
  "name": "legal-admin-system-api",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "JWT_SECRET": "@jwt_secret",
    "DATABASE_URL": "@database_url"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
