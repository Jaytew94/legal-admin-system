{
  "version": 2,
  "builds": [
    {
      "src": "legal.consulargo.io/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/legal.consulargo.io/frontend/build/index.html"
    }
  ]
}