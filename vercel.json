{
  "version": 2,
  "functions": {
    "api/hello.ts": {
      "runtime": "@vercel/node"
    },
    "api/auth/login.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/hello",
      "dest": "/api/hello.ts"
    },
    {
      "src": "/api/auth/login",
      "dest": "/api/auth/login.ts"
    }
  ]
}
