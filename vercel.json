{
  "version": 2,
  "functions": {
    "api/hello.ts": {
      "runtime": "@vercel/node"
    },
    "api/auth/login.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/hello",
      "dest": "/api/hello.ts"
    },
    {
      "src": "/api/auth/login",
      "dest": "/api/auth/login.ts"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
